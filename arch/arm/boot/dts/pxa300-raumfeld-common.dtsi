#include "pxa3xx.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/interrupt-controller/irq.h>

/ {
	chosen {
		bootargs = "console=ttyS0,115200 ubi.mtd=3 root=/dev/nfs nfsroot=192.168.23.1:/export/rootfs-pxa3xx,tcp,v3,nolock rw ip=192.168.23.2:::255.255.255.0 debug init=/init.sh";
		stdout-path = &ffuart;
	};

	memory {
		reg = <0xa0000000 0x8000000>;	/* 128 MB */
	};

	reg_3v3: voltage_3v3 {
		compatible = "regulator-fixed";
		regulator-name = "3v3-fixed-supply";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
	};

	reg_1v8: voltage_1v8 {
		compatible = "regulator-fixed";
		regulator-name = "1v8-fixed-supply";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
	};

	reg_va_5v0: voltage_va_5v0 {
		compatible = "regulator-fixed";
		regulator-name = "va-5v0-fixed-supply";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		gpio = <&gpio 124 GPIO_ACTIVE_HIGH>;
	};

	xo_27mhz: xo_27mhz {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <27000000>;
		clock-accuracy = <100>;
	};

	ssp_dai0: ssp_dai@0 {
		compatible = "mrvl,pxa-ssp-dai";
		pinctrl-names = "default";
		pinctrl-0 = <&ssp0_dai_pins>;
		port = <&ssp1>;
		#sound-dai-cells = <0>;
		dmas = <&pdma 13 3
			&pdma 14 3>;
		dma-names = "rx", "tx";
	};

	ssp_dai1: ssp_dai@1 {
		compatible = "mrvl,pxa-ssp-dai";
		pinctrl-names = "default";
		pinctrl-0 = <&ssp1_dai_pins>;
		port = <&ssp2>;
		#sound-dai-cells = <0>;
		dmas = <&pdma 15 3
			&pdma 16 3>;
		dma-names = "rx", "tx";
	};

	spi: spi {
		status = "okay";
		compatible = "spi-gpio";
		#address-cells = <0x1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&spi_pins>;

		gpio-sck = <&gpio 95 GPIO_ACTIVE_HIGH>;
		gpio-miso = <&gpio 98 GPIO_ACTIVE_HIGH>;
		gpio-mosi = <&gpio 97 GPIO_ACTIVE_HIGH>;
		cs-gpios = <
			&gpio 34 GPIO_ACTIVE_HIGH
			&gpio 125 GPIO_ACTIVE_HIGH
			&gpio 96 GPIO_ACTIVE_HIGH
		>;
		num-chipselects = <3>;
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&led_pins>;

		led@1 {
			label = "raumfeld:1";
			gpios = <&gpio 36 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "heartbeat";
		};

		led@2 {
			label = "raumfeld:2";
			gpios = <&gpio 35 GPIO_ACTIVE_HIGH>;
		};
	};

	gpio-poweroff {
		compatible = "gpio-poweroff";
		gpios = <&gpio 16 GPIO_ACTIVE_HIGH>;
		timeout-ms = <0>;
	};
};

&ffuart {
	status = "okay";
};

&pwri2c {
	status = "okay";

	max8660: max8660@34 {
		compatible = "maxim,max8660";
		reg = <0x34>;

		regulators {
			regulator@0 {
				regulator-compatible= "V3(DCDC)";
				regulator-min-microvolt = <725000>;
				regulator-max-microvolt = <1800000>;
			};

			regulator@1 {
				regulator-compatible= "V4(DCDC)";
				regulator-min-microvolt = <725000>;
				regulator-max-microvolt = <1800000>;
			};

			regulator@2 {
				regulator-compatible= "V5(LDO)";
				regulator-min-microvolt = <1700000>;
				regulator-max-microvolt = <2000000>;
			};

			reg_vcc_sdio: regulator@3 {
				regulator-compatible= "V6(LDO)";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
			};

			regulator@4 {
				regulator-compatible= "V7(LDO)";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <3300000>;
			};
		};
	};
};

&ssp1 {
	status = "okay";
};

&ssp2 {
	status = "okay";
};

&pxai2c1 {
	status = "okay";
	mrvl,i2c-fast-mode;
	pinctrl-names = "default";
	pinctrl-0 = <&pxai2c1_pins>;
};

&nand_controller {
	//status = "okay";

	nand@0 {
		reg = <0>;
		nand-rb = <0>;
		nand-ecc-mode = "hw";
		marvell,nand-keep-config;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "Bootloader";
				reg = <0x0000000 0xa0000>;
				read-only;
			};

			partition@a0000 {
				label = "BootloaderEnvironment";
				reg = <0x0a0000 0x20000>;
			};

			partition@c0000 {
				label = "BootloaderSplashScreen";
				reg = <0x0c0000 0x60000>;
			};

			partition@120000 {
				label = "UBI";
				reg = <0x120000 0x7ee0000>;
			};
		};
	};
};

&pxa3xx_ohci {
	status = "okay";
	marvell,enable-port1;
	marvell,port-mode = <2>; /* PMM_GLOBAL_MODE */
	marvell,power-budget = <200>;
	pinctrl-names = "default";
	pinctrl-0 = <&pxa3xx_ohci_pins>;
};

&mmc0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&mmc0_pins>;
	mrvl,detect-delay-ms = <200>;
	vmmc-supply = <&reg_vcc_sdio>;
};

&pinctrl {
	led_pins: led_pins {
		pinctrl-single,pins = <
			MFP_PIN_PXA300(36) MFP_AF0
			MFP_PIN_PXA300(35) MFP_AF0
		>;
		pinctrl-single,low-power-mode = MFP_LPM(MFP_LPM_FLOAT);
	};

	pxai2c1_pins: pxai2c1_pins {
		pinctrl-single,pins = <
			MFP_PIN_PXA300(21) MFP_AF1	/* SCL	*/
			MFP_PIN_PXA300(22) MFP_AF1	/* SDA	*/
		>;
		pinctrl-single,low-power-mode = MFP_LPM(MFP_LPM_PULL_HIGH);
	};

	spi_pins: spi_pins {
		pinctrl-single,pins = <
			MFP_PIN_PXA300(95) MFP_AF0	/* SCK	*/
			MFP_PIN_PXA300(97) MFP_AF0	/* MOSI	*/
			MFP_PIN_PXA300(98) MFP_AF0	/* MISO	*/
			MFP_PIN_PXA300(34) MFP_AF0	/* CS#0	*/
			MFP_PIN_PXA300(125) MFP_AF0	/* CS#1	*/
			MFP_PIN_PXA300(96) MFP_AF0	/* CS#2	*/
		>;
		pinctrl-single,low-power-mode = MFP_LPM(MFP_LPM_PULL_LOW);
	};

	pxa3xx_ohci_pins: pxa3xx_ohci_pins {
		pinctrl-single,pins = <
			MFP_PIN_PXA300_0_2 MFP_AF1	/* USBHPEN */
			MFP_PIN_PXA300_1_2 MFP_AF1	/* USBHPWR */
		>;
		pinctrl-single,low-power-mode = MFP_LPM(MFP_LPM_PULL_LOW);
	};

	mmc0_pins: mmc0_pins {
		pinctrl-single,pins = <
			MFP_PIN_PXA300(3) MFP_AF4	/* DAT0	*/
			MFP_PIN_PXA300(4) MFP_AF4	/* DAT1	*/
			MFP_PIN_PXA300(5) MFP_AF4	/* DAT2	*/
			MFP_PIN_PXA300(6) MFP_AF4	/* DAT3	*/
			MFP_PIN_PXA300(7) MFP_AF4	/* CLK	*/
			MFP_PIN_PXA300(8) MFP_AF4	/* CMD	*/
		>;
		pinctrl-single,low-power-mode = MFP_LPM(MFP_LPM_FLOAT);
		// XXX drive high?
	};

	ssp0_dai_pins: ssp0_dai_pins {
		pinctrl-single,pins = <
			MFP_PIN_PXA300(85) MFP_AF1	/* SCLK		*/
			MFP_PIN_PXA300(86) MFP_AF1	/* FRM		*/
			MFP_PIN_PXA300(87) MFP_AF1	/* TXD		*/
			MFP_PIN_PXA300(88) MFP_AF1	/* RXD		*/
			MFP_PIN_PXA300(89) MFP_AF1	/* EXTCLK	*/
			MFP_PIN_PXA300(90) MFP_AF1	/* SYSCLK	*/
		>;
		pinctrl-single,low-power-mode = MFP_LPM(MFP_LPM_PULL_LOW);
	};

	ssp1_dai_pins: ssp1_dai_pins {
		pinctrl-single,pins = <
			MFP_PIN_PXA300(25) MFP_AF2	/* SCLK		*/
			MFP_PIN_PXA300(26) MFP_AF2	/* FRM		*/
			MFP_PIN_PXA300(27) MFP_AF2	/* TXD		*/
			MFP_PIN_PXA300(29) MFP_AF2	/* EXTCLK	*/
		>;
		pinctrl-single,low-power-mode = MFP_LPM(MFP_LPM_PULL_LOW);
	};
};

