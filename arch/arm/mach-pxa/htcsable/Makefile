                                    htcsable_ts2.o \
                                    htcsable_pm.o \
                                    htcsable_core.o \
				    htcsable_keyboard.o \
                                    htcsable_buttons.o \
                                    htcsable_kbd.o

obj-$(CONFIG_HTCSABLE_BT)		+= htcsable_bt.o
obj-$(CONFIG_HTCSABLE_PHONE)		+= htcsable_phone.o
obj-$(CONFIG_HTCSABLE_ROM)		+= htcsable_rom.o
obj-$(CONFIG_HTCSABLE_LCD)		+= htcsable_lcd.o
obj-$(CONFIG_HTCSABLE_BACKLIGHT)	+= htcsable_bl.o

